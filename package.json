  "repository": {
    "type": "git",
    "url": "https://github.com/danielnarey/danielnarey.com"
  },
  "type": "module",
  "scripts": {
    "build": "del dist/* && svelte-render",
    "build:dev": "del dist/* && svelte-render --development",
    "merge:next": "git checkout master && git pull && git merge next -s recursive -Xtheirs",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "build",
    "update": "updtr"
  },
  "dependencies": {
    "@jackfranklin/rollup-plugin-markdown": "^0.2.0",
    "@metamodern/design-system": "^1.1.1",
    "svelte-preprocess": "^3.7.1",
    "tailwindcss": "^1.2.0",
    "typeface-barlow": "0.0.71",
    "typeface-barlow-semi-condensed": "0.0.71",
    "typeface-source-code-pro": "^1.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@metamodern/svelte-render": "^1.0.0",
    "core-js": "^3.6.5",
    "del-cli": "^3.0.0",
    "pug": "^2.0.4",
    "sugarss": "^2.0.0",
    "svelte": "^3.22.3",
    "updtr": "^3.1.0"
  },
  "engines": {
    "node": ">=12"
  }
